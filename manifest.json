{
  "manifest_version": 2,

  "name": "HostSwitcher",
  "options_page": "options.html",
  "description": "Easily switch between hosts for the same path/endpoint.",
  "version": "1.2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
